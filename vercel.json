{
    "version": 2,
    "builds": [
      {
        "src": "Inventory_management_backend/wsgi.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Inventory_management_backend/wsgi.py"
      }
    ]
  }
  